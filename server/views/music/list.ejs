  <!-- Bootstrap 5.3.2 CDN -->
  <!-- Latest compiled and minified CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <!-- Latest compiled JavaScript -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
  
<main class = "container">
      <div class="row">
        <div class="col-md-offset-3">
        <h1><%= title %></h1>
            <a href="/music/add" class="btn btn-primary" 
            role="button"><i class="fas fa-plus-circle"></i> Add Song</a>
        <br>
        <!--This is the table it has and has the edit button and delete button -->
      <table class= "table table-bordered table-striped table-hover">
      <tr class="d-flex">
        <th class ="text-center col-2">Title</th>
        <th class ="text-center col-2">Artist</th>
        <th class ="text-center col-2">Album</th>
        <th class ="text-center col-2">Year</th>
        <th class ="text-center col-2">Length</th>
        <th class ="text-center col-2">Edit</th>
        </td>
        <% for(let count=0;count<MusicList.length;count++) { %>
          <tr class="d-flex">
            <td class ="text-center col-2"><%= MusicList[count].name %></td>
            <td class ="text-center col-2"><%= MusicList[count].artist %></td>
            <td class ="text-center col-2"><%= MusicList[count].album %></td>
            <td class ="text-center col-2"><%= MusicList[count].year %></td>
            <td class ="text-center col-2"><%= MusicList[count].length %></td>
            <td class ="text-center col-1"><a href="/music/edit/<%= MusicList[count]._id %>" class="btn btn-warning btn-sm"> Edit </a></td>
            <td class ="text-center col-1"><a href="/music/delete/<%= MusicList[count]._id %>" class="btn btn-warning btn-sm"> Delete </a></td>
          </tr>
        <% } %>
        </tr>
        </table>
        </div>
    </div>
    </div>
    </main>

